@model IEnumerable<CS2ADanielDeLara_MVCPROJECT.BusLogic.Model.tblStudent>

<div>
    <h2>Student List</h2>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
        @foreach (var student in Model)
            {
                <tr>
                    <td>@student.FirstName</td>
                    <td>@student.LastName</td>
                    <td>@student.Age</td>
                    <td>@student.Course</td>
                </tr>    
            }
    </table>
</div>